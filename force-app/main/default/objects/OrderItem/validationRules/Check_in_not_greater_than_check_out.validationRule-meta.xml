<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_in_not_greater_than_check_out</fullName>
    <active>true</active>
    <errorConditionFormula>NULLVALUE(Check_Out_Quantity__c, 0)  &lt;  NULLVALUE(Total_Check_In_Quantity__c, 0)</errorConditionFormula>
    <errorMessage>Total Checked In Quantity cannot be greater than Checked Out Quantity.</errorMessage>
</ValidationRule>
